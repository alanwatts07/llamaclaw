{
  "agents": {
    "defaults": {
      "model": {
        "primary": "ollama/qwen3:32b"
      },
      "heartbeat": {
        "every": "30m",
        "target": "none",
        "prompt": "Read HEARTBEAT.md if it exists (workspace context). Follow it strictly. Do not infer or repeat old tasks from prior chats. If nothing needs attention, reply HEARTBEAT_OK."
      },
      "compaction": {
        "mode": "safeguard"
      }
    },
    "list": [
      {
        "id": "main",
        "default": true
      }
    ]
  },
  "models": {
    "mode": "merge",
    "providers": {
      "ollama": {
        "baseUrl": "https://host.docker.internal:11434/v1",
        "apiKey": "ollama",
        "api": "openai-completions",
        "models": [
          {
            "id": "qwen3:32b",
            "name": "Qwen3 32B",
            "reasoning": false,
            "input": ["text"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 32768,
            "maxTokens": 8192
          }
        ]
      }
    }
  },
  "tools": {
    "profile": "minimal",
    "allow": ["exec"],
    "exec": {
      "host": "gateway",
      "security": "full"
    }
  },
  "channels": {},
  "gateway": {
    "mode": "local",
    "auth": {
      "mode": "token",
      "token": "llamaclaw-local-token"
    }
  }
}
